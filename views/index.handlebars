<div class="short-header-page wsite-theme-light wsite-page-index full-width-on touch">
  <div id="header-wrap">
    <label class="nav-trigger hamburger" for="nav-trigger">
      <span class="open-btn">
        <span class="mobile">
        </span>
        <span class="mobile">
        </span>
        <span class="mobile">
        </span>
      </span>
    </label>
    <div id="logo">
      <span class="wsite-logo">
        <a href="/">
          <img alt="Cause For SB Paws" src="/assets/img/logo1.jpg">
          </img>
        </a>
      </span>
    </div>
  </div>
  {{> navbar }}
  <div id="banner-wrap">
    <div class="home_background">
    </div>
  </div>
  <div id="main-wrap">
    <h2 class="wsite-content-title" style="margin-top: 5px;">
      JOIN THE CAUSE FOR SB PAWS!
    </h2>
    <h2>
      Recently adopted dogs:
    </h2>
    <div>
      <div>
        <ul id="dog-container">
          {{#each dogs}} {{#if adopted}}
          <div id="dog-photo-block">
            <li id="dog-block">
              {{this.name}}
            </li>
            <img id="dog-photo" src="{{this.photo}}">
            </img>
          </div>
          {{/if}} {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

